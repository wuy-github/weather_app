{
  "version": 8,
  "name": "Raster OSM",
  "sources": {
    "osm-tiles": {
      "type": "raster",
      "tiles": [
        "https://tile.openstreetmap.org/{z}/{x}/{y}.png"
      ],
      "tileSize": 256,
      "minzoom": 0,
      "maxzoom": 19
    }
  },
  "layers": [
    {
      "id": "background",
      "type": "background",
      "paint": { "background-color": "#ffffff" }
    },
    {
      "id": "osm-tiles",
      "type": "raster",
      "source": "osm-tiles"
    }
  ]
}
